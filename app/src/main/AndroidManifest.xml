<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rishabh.symphony">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/symphony"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:largeHeap="false"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="._Fragment1_" />
        <activity android:name="._Fragment2_" />
        <activity android:name="._Fragment3_" />
        <activity android:name="._Fragment4_"></activity>
    </application>

</manifest>
